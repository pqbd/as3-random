package test.alx.common.util{  import alx.common.util.IRandom  import alx.common.util.CFakeRandom  public class CFakeRandomUnitTest extends CAbstractRandomUnitTest  {    public function CFakeRandomUnitTest( strLabel:String):void    {      super( strLabel);    }    protected override function createRandom( nSeed:uint = 0):IRandom    {      return new CFakeRandom( nSeed);    }    protected override function testList():Array    {      return super.testList().concat( new Array( this.testAsk));    }    public function testAsk()    {      var random:CFakeRandom = this.createRandom() as CFakeRandom;      random.ask( 0.9, 0, 0.3, 0.5, 0.499999999);      this.getTester().isEqual( 'random.nextBoolean()'                              , random.nextBoolean()                              , false                              );      this.getTester().isEqual( 'random.nextBoolean()'                              , random.nextBoolean()                              , true                              );      this.getTester().isEqual( 'random.nextBoolean()'                              , random.nextBoolean()                              , true                              );      this.getTester().isEqual( 'random.nextBoolean()'                              , random.nextBoolean()                              , false                              );      this.getTester().isEqual( 'random.nextBoolean()'                              , random.nextBoolean()                              , true                              );      try      {        random.ask( 0.2, -0.3, 0.5);        this.getTester().notHere( 'random.ask( 0.2, -0.3, 0.5)');      }      catch( e:Error){}      try      {        random.ask( 0.2, 6, 0.5);        this.getTester().notHere( 'random.ask( 0.2, 6, 0.5)');      }      catch( e:Error){}      try      {        random.ask( 1);        this.getTester().notHere( 'random.ask( 1)');      }      catch( e:Error){}    }  }}